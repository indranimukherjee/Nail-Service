<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    @* template header Files *@
    <meta name="keywords" content="" />
    <meta name="description" content="" />

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!--<link rel="shortcut icon" href="PUT YOUR FAVICON HERE">-->
    <!-- Google Web Font Embed -->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,600italic,700,700italic,800,800italic' rel='stylesheet' type='text/css'>

    <!-- Bootstrap core CSS -->
    <link href="@Url.Content("~/Content/mytemplate/")css/bootstrap.css" rel='stylesheet' type='text/css'>

    <!-- Custom styles for this template -->
    <link href="@Url.Content("~/Content/mytemplate/")js/colorbox1/colorbox.css" rel='stylesheet' type='text/css'>
    <link href="@Url.Content("~/Content/mytemplate/")css/templatemo_style.css" rel='stylesheet' type='text/css'>

    <script src="@Url.Content("~/Content/mytemplate/")js/jquery.min.js" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/mytemplate/")js/bootstrap.min.js" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/mytemplate/")js/stickUp.min.js" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/mytemplate/")js/colorbox1/jquery.colorbox-min.js" type="text/javascript"></script>
    @*<script src="@Url.Content("~/Content/mytemplate/")js/templatemo_script.js" type="text/javascript"></script>*@

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    @*[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]*@
 


    <link rel="icon" type="image/ico" href="~/Content/mytemplate/Images/favicon.ico">
</head>
<body>
    <div class="templatemo-top-bar" id="templatemo-top">
        <div class="container">
            <div class="subheader">
                <div id="phone" class="pull-left">
                    <img src="@Url.Content("~/Content/mytemplate/")images/phone.png" alt="phone" />
                    010-020-0340
                </div>
                @*<span class="social-icon-fb"></span>*@
                <div id="email" class="pull-right">
                    <img src="@Url.Content("~/Content/mytemplate/")images/email.png" alt="email" />
                    contact@website.com
                </div>
            </div>
        </div>
    </div>
    <div class="templatemo-top-menu">
        <div class="container">
            <!-- Static navbar -->
            <div class="navbar navbar-default" role="navigation">
                <div class="container">
                   
                    <div class="navbar-collapse collapse" id="templatemo-nav-bar">
                        <a href="#"><img src="@Url.Content("~/Content/mytemplate/")images/logo.gif" class="logo" height="57" alt="Urbanic template" title="Urbanic Template" /></a>

                        <ul class="nav navbar-nav navbar-right" style="margin-top: 40px;  font-style: oblique; font-size: small; font-family: cursive;">
                            <li>@Html.ActionLink("HOME", "Index", "Home")</li>
                            <li>@Html.ActionLink("SERVICES", "Index", "Services")</li>
                            @*<li>@Html.ActionLink("SERVICES", "service", "Home")</li>*@
                            <li>@Html.ActionLink("NAIL GALLERY", "gallery", "Home")</li>
                            <li>@Html.ActionLink("BOOKING", "Index", "bookings")</li>
                            <li>@Html.ActionLink("CONTACT US", "Index", "visitors")</li>
                            <li>@Html.ActionLink("PAYMENT", "Payment", "Home")</li>                 
                          
                        </ul>
                    </div><!--/.nav-collapse -->
                </div><!--/.container-fluid -->
            </div><!--/.navbar -->
        </div> <!-- /container -->
    </div>


    <div>
        @RenderBody()
    </div>

    @Html.Partial("FooterPartial")
   
</body>
</html>
